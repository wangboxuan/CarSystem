    <style type="text/css">
        .grid {
            box-shadow: 1px 1px 1px black;
            padding: 10px;
            height: 200px;
            margin-bottom: 10px;
            border: 1px solid #ccc;
            overflow: hidden;
        }

        .grid img {
            float: left;
            width: 70px;
            height: 70px;
        }

        .grid h3 {
            float: left;
            font-size: 14px;
            width: 100px;
            height: 30px;
            overflow: hidden;
            font-weight: bold;
        }

        .grid p {
            clear: both;
            padding: 10px;
        }
        body {
            width: 100%;
            background: url(bsj.jpg) no-repeat;
            background-size:cover;
        }
        h1{
            text-align: center;
            color:#fff;
        }
    </style>
</head>

<body>
    <nav class="navbar navbar-default">
        <div class="container-fluid">
            <!-- Brand and toggle get grouped for better mobile display -->
